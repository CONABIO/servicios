<p id="notice"><%= notice %></p>

<h1>Naturalista Estadisticas</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Icono</th>
      <th>Descripcion</th>
      <th>Lugar</th>
      <th>Numero especies</th>
      <th>Numero observaciones</th>
      <th>Numero observadores</th>
      <th>Numero identificadores</th>
      <th>Numero miembros</th>
      <th>Estado</th>
      <th>Tipo lugar</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @naturalista_estadisticas.each do |naturalista_estadistica| %>
      <tr>
        <td><%= naturalista_estadistica.titulo %></td>
        <td><%= naturalista_estadistica.icono %></td>
        <td><%= naturalista_estadistica.descripcion %></td>
        <td><%= naturalista_estadistica.lugar_id %></td>
        <td><%= naturalista_estadistica.numero_especies %></td>
        <td><%= naturalista_estadistica.numero_observaciones %></td>
        <td><%= naturalista_estadistica.numero_observadores %></td>
        <td><%= naturalista_estadistica.numero_identificadores %></td>
        <td><%= naturalista_estadistica.numero_miembros %></td>
        <td><%= naturalista_estadistica.estado %></td>
        <td><%= naturalista_estadistica.tipo_lugar %></td>
        <td><%= link_to 'Show', naturalista_estadistica %></td>
        <td><%= link_to 'Edit', edit_naturalista_estadistica_path(naturalista_estadistica) %></td>
        <td><%= link_to 'Destroy', naturalista_estadistica, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Naturalista Estadistica', new_naturalista_estadistica_path %>
