<div id="eventos-container" class="mb-3">
  <form action="<%= new_calendario_evento_url %>">
    <div class="d-flex jumbotron">
      <input id="name" type="text" class="form-control form-control-lg mx-3" placeholder="Usuario">
      <input id="passwd" type="password" class="form-control form-control-lg mx-3" placeholder="Contraseña">
      <button type="submit" class="btn btn-success btn-lg btn-block mx-5">Inicia sesión</button>
    </div>
  </form>
</div>
